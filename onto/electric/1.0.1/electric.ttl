              rdfs:domain :EVSE ;
              rdfs:range :Connector ;
              rdfs:comment "Associa un Connettore ad una Colonnina di Ricarica per Veicoli Elettrici (EVSE)."@it ,
                           "Associates a Connector to an Electric Vehicle Supply Equipment (EVSE)."@en ;
              rdfs:label "ha un connettore"@it ,
