###  http://www.w3.org/2004/02/skos/core#inScheme
<http://www.w3.org/2004/02/skos/core#inScheme> rdf:type owl:ObjectProperty .


###  http://www.w3id.org/urban-iot/electric#EVSEState
:EVSEState rdf:type owl:ObjectProperty ;
           rdfs:domain :EVSERecord ;
           rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                               owl:hasValue <http://w3id.org/urban-iot/kos/electric/EVSE-state>
                                             ]
                                           ) ;
                        rdf:type owl:Class
                      ] ;
           rdfs:comment "Associazioni a possibili stati  in cui una Colonnina per la Ricarica Veicoli Elettrici (EVSE) può trovarsi."@it ,
                        "Connection to possible states for an EVSE (Electric Vehicle Supply Equipment)."@en ;
           rdfs:label "EVSE state"@en ,
                      "stato dell'EVSE"@it .


###  http://www.w3id.org/urban-iot/electric#chargingEnabledBy
:chargingEnabledBy rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://www.w3id.org/urban-iot/core#sessionEnabledBy> ;
                   owl:inverseOf :enablesCharging ;
                   rdfs:domain :ChargingSession ;
                   rdfs:range :ChargingService ;
                   rdfs:comment "Associa una Stazione di Ricarica ad un Servizio di Ricarica che l'abilita."@it ,
                                "Associates a Charging Station to a Charging Service which enabled it."@en ;
                   rdfs:label "charging enabled by"@en ,
                              "ricarica abilitata da"@it .


###  http://www.w3id.org/urban-iot/electric#chargingPerformedBy
:chargingPerformedBy rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://www.w3id.org/urban-iot/core#sessionPerformedBy> ;
                     owl:inverseOf :performsCharging ;
                     rdfs:domain :ChargingSession ;
                     rdfs:range <http://www.w3id.org/urban-iot/core#ServicePrivateUser> ;
                     rdfs:comment "Associa una Sessione di Ricarica all'Utente Privato del Servizio che la esegue."@it ,
                                  "Associates a Charging Session with a Service Private User who performs the Session."@en ;
                     rdfs:label "charging performed by"@en ,
                                "ricarica effettuata da"@it .


###  http://www.w3id.org/urban-iot/electric#chargingStationUsedIn
:chargingStationUsedIn rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://www.w3id.org/urban-iot/core#resourceUsedIn> ;
                       owl:inverseOf :usesChargingStation ;
                       rdfs:domain :ChargingStation ;
                       rdfs:range :ChargingSession ;
                       rdfs:comment "Associa una Stazione di Ricarica ad una Sessione di Ricarica che la utilizza."@it ,
                                    "Associates a Charging Station to a Charging Session where the Station is used."@en ;
                       rdfs:label "charging station used in"@en ,
                                  "stazione di ricarica usata in"@it .


###  http://www.w3id.org/urban-iot/electric#enablesCharging
:enablesCharging rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://www.w3id.org/urban-iot/core#enablesSession> ;
                 rdfs:domain :ChargingService ;
                 rdfs:range :ChargingSession ;
                 rdfs:comment "A Charging Service can enable Charging Sessions"@en ,
                              "Un Servizio di Ricarica abilita Sessioni di Ricarica."@it ;
                 rdfs:label "abilita la ricarica"@it ,
                            "enables charging"@en .


###  http://www.w3id.org/urban-iot/electric#hasChargeAccessMethod
