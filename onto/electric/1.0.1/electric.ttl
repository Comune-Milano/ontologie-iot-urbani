                       rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                           owl:hasValue <http://w3id.org/urban-iot/kos/electric/charge-access-method>
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
                       rdfs:comment "Association for an Electric Vehicle Supply Equipment (EVSE) to possible methods to activate the charge."@en ,
                                    "Associazione di una Colonnina di Ricarica per Veicoli Elettrici (EVSE) a possibili metodi per attivare la ricarica."@it ;
                       rdfs:label "ha un metodo di accesso alla ricarica"@it ,
                                  "has charge access method"@en .


###  http://www.w3id.org/urban-iot/electric#hasChargingServiceOffer
:hasChargingServiceOffer rdf:type owl:ObjectProperty ;
                         rdfs:domain :ChargingStation ;
                         rdfs:range :ChargingServiceOffer ;
                         rdfs:comment "Associates a Charging Station to a Charging Offer."@en ,
                                      "Associazine tra una Stazione di Ricarica e un'Offerta di Ricarica."@it ;
                         rdfs:label "ha un'offerta di ricarica"@it ,
                                    "has charging service offer"@en .


###  http://www.w3id.org/urban-iot/electric#hasChargingStationOwner
:hasChargingStationOwner rdf:type owl:ObjectProperty ;
                         rdfs:domain :ChargingStation ;
                         rdfs:range <https://schema.org/Organization> ;
                         rdfs:comment "Associa una Organizzazione a una Stazione di Ricarica, dove l'Organizzazione possiede la Stazione."@it ,
                                      "Associates an Organization with a Charging Station, where the Organization owns the Station."@en ;
                         rdfs:label "has charging station owner"@en ,
                                    "possiede una stazione di ricarica"@it .


###  http://www.w3id.org/urban-iot/electric#hasConnector
:hasConnector rdf:type owl:ObjectProperty ;
