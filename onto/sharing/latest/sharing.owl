#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#inScheme
<http://www.w3.org/2004/02/skos/core#inScheme> rdf:type owl:ObjectProperty .


###  http://www.w3id.org/urban-iot/sharing#currentSharingStation
uiots:currentSharingStation rdf:type owl:ObjectProperty ;
                            rdfs:domain uiots:SharingMobilityVehicleRecord ;
                            rdfs:range uiots:SharingStation ;
                            rdfs:comment "Property referring to the Sharing Station in which the vehicle is currently located."@en ,
                                         "Proprietà che indica la Sharing Station in cui si trova attualmente il veicolo."@it ;
                            rdfs:label "current sharing station"@en ,
                                       "stazione di sharing attuale"@it .


###  http://www.w3id.org/urban-iot/sharing#dockState
uiots:dockState rdf:type owl:ObjectProperty ;
                rdfs:domain uiots:DockRecord ;
                rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                    owl:hasValue sh-kos:dock-state
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
                rdfs:comment "Property referring to the current state of a Dock."@en ,
                             "Proprietà che indica lo stato in cui si trova attualmente uno stallo."@it ;
                rdfs:label "dock state"@en ,
                           "stato dello stallo"@it .


###  http://www.w3id.org/urban-iot/sharing#enablesTrip
uiots:enablesTrip rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf uiot:enablesSession ;
                  owl:inverseOf uiots:tripEnabledBy ;
                  rdfs:domain uiots:SharingMobilityService ;
                  rdfs:range uiots:SharingMobilityTrip ;
                  rdfs:comment "Associa un Servizio di Sharing Mobility ad un Noleggio abilitato dal servizio."@it ,
                               "Associates a Sharing Service with a Sharing Mobility Trip enabled by the service."@en ;
                  rdfs:label "abilita noleggio"@it ,
                             "enables trip"@en .


###  http://www.w3id.org/urban-iot/sharing#hasCarSupply
uiots:hasCarSupply rdf:type owl:ObjectProperty ;
                   rdfs:domain uiots:Car ;
                   rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                       owl:hasValue vh-kos:car-supply
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
                   rdfs:comment "Associa una Autovettura alla sua tipologia di alimentazione."@it ,
                                "Associates a Car with its supply typology."@en ;
                   rdfs:label "ha alimentazione autovettura"@it ,
                              "has car supply"@en .


###  http://www.w3id.org/urban-iot/sharing#hasContractualOperationArea
uiots:hasContractualOperationArea rdf:type owl:ObjectProperty ;
                                  rdfs:domain schema:Service ;
                                  rdfs:range uiots:ContractualOperationArea ;
                                  rdfs:comment "Property referring to the contractual operation area of the Service."@en ,
                                               "Proprietà che identifica i confini minimi di esercizio di un Servizio."@it ;
                                  rdfs:label "ha confini di esercizio minima"@it ,
                                             "has contractual operation area"@en .


###  http://www.w3id.org/urban-iot/sharing#hasDock
uiots:hasDock rdf:type owl:ObjectProperty ;
              owl:inverseOf uiots:isDockOf ;
              rdfs:domain uiots:PhysicalSharingStation ;
              rdfs:range uiots:Dock ;
              rdfs:comment "Associa una Stazione di Sharing ad uno Stallo contenuto nella stazione."@it ,
                           "Associates a Sharing Station with a Dock in the station."@en ;
              rdfs:label "ha stallo"@it ,
                         "has dock"@en .


###  http://www.w3id.org/urban-iot/sharing#hasDockAbilitated
uiots:hasDockAbilitated rdf:type owl:ObjectProperty ;
                        rdfs:domain uiots:SharingMobilityVehicle ;
                        rdfs:range uiots:DockKind ;
                        rdfs:comment "Associa un Veicolo di Sharing Mobility alle tipologie di stallo supportate dal veicolo."@it ,
                                     "Associates a Sharing Mobility Vehicle with a Dock Kind supported by the vehicle."@en ;
                        rdfs:label "ha tipologia di stallo abilitato"@it ,
                                   "has dock kind abilitated"@en .


###  http://www.w3id.org/urban-iot/sharing#hasDockKind
uiots:hasDockKind rdf:type owl:ObjectProperty ;
                  rdfs:domain uiots:Dock ;
                  rdfs:range uiots:DockKind ;
                  rdfs:comment "Associa uno Stallo alla sua tipologia."@it ,
                               "Associates a Dock with its typology."@en ;
                  rdfs:label "ha tipologia di stallo"@it ,
                             "has dock kind"@en .


###  http://www.w3id.org/urban-iot/sharing#hasEmissionStandard
uiots:hasEmissionStandard rdf:type owl:ObjectProperty ;
                          rdfs:domain schema:Vehicle ;
                          rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                              owl:hasValue vh-kos:european-emission-standard
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                          rdfs:comment "Associa un Veicolo al suo livello di emissioni inquinanti secondo gli standard definiti,"@it ,
                                       "Associates a Vehicle with the emission standard that complies with."@en ;
                          rdfs:label "ha standard emissioni"@it ,
                                     "has emission standard"@en .


###  http://www.w3id.org/urban-iot/sharing#hasEndingStation
uiots:hasEndingStation rdf:type owl:ObjectProperty ;
                       rdfs:domain uiots:SharingMobilityTrip ;
                       rdfs:range uiots:SharingStation ;
                       rdfs:comment "Associa un Noleggio alla Stazione di Sharing in cui viene terminato."@it ,
                                    "Associates a Sharing Mobility Trip with the Sharing Station in which it ends."@en ;
                       rdfs:label "ha stazione di fine"@it ,
                                  "has ending station"@en .


###  http://www.w3id.org/urban-iot/sharing#hasForbiddenDropOffArea
uiots:hasForbiddenDropOffArea rdf:type owl:ObjectProperty ;
                              rdfs:domain uiots:SharingMobilityService ;
                              rdfs:range uiots:ForbiddenDropOffArea ;
                              rdfs:comment "Property referring to the forbidden drop-off area of the Sharing Mobility Service."@en ,
                                           "Proprietà che identifica l'area di rilascio vietato di un Servizio di Sharing Mobility."@it ;
                              rdfs:label "ha area rilascio vietato"@it ,
                                         "has forbidden drop-off area"@en .


###  http://www.w3id.org/urban-iot/sharing#hasOperationArea
uiots:hasOperationArea rdf:type owl:ObjectProperty ;
                       rdfs:domain schema:Service ;
                       rdfs:range uiots:OperationArea ;
                       rdfs:comment "Property referring to the operation area of the Service."@en ,
                                    "Proprietà che identifica l'area operativa di un Servizio."@it ;
                       rdfs:label "ha area operativa"@it ,
                                  "has operation area"@en .


###  http://www.w3id.org/urban-iot/sharing#hasPickUpDropOffArea
uiots:hasPickUpDropOffArea rdf:type owl:ObjectProperty ;
                           rdfs:domain uiots:SharingMobilityService ;
                           rdfs:range uiots:PickUpDropOffArea ;
                           rdfs:comment "Property referring to the pick-up drop-off area of the Sharing Mobility Service."@en ,
                                        "Proprietà che identifica l'area di prelievo/rilascio di un Servizio di Sharing Mobility."@it ;
                           rdfs:label "ha area di prelievo rilascio"@it ,
                                      "has pick-up drop-off area"@en .


###  http://www.w3id.org/urban-iot/sharing#hasPreferredDropOffArea
uiots:hasPreferredDropOffArea rdf:type owl:ObjectProperty ;
                              rdfs:domain uiots:SharingMobilityService ;
                              rdfs:range uiots:PreferredDropOffArea ;
                              rdfs:comment "Property referring to the preferred drop-off area of the Sharing Mobility Service."@en ,
                                           "Proprietà che identifica l'area preferenziale di rilascio di un Servizio di Sharing Mobility."@it ;
                              rdfs:label "ha area preferenziale rilascio"@it ,
                                         "has preferred drop-off area"@en .


###  http://www.w3id.org/urban-iot/sharing#hasPreferredPickUpArea
uiots:hasPreferredPickUpArea rdf:type owl:ObjectProperty ;
                             rdfs:domain uiots:SharingMobilityService ;
                             rdfs:range uiots:PreferredPickUpArea ;
                             rdfs:comment "Property referring to the preferred pick-up area of the Sharing Mobility Service."@en ,
                                          "Proprietà che identifica l'area preferenziale di prelievo di un Servizio di Sharing Mobility."@it ;
                             rdfs:label "ha area preferenziale prelievo"@it ,
                                        "has preferred pick-up area"@en .


###  http://www.w3id.org/urban-iot/sharing#hasPropulsionKind
uiots:hasPropulsionKind rdf:type owl:ObjectProperty ;
                        rdfs:domain schema:Vehicle ;
                        rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                            owl:hasValue vh-kos:propulsion-kind
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
                        rdfs:comment "Associa un Veicolo alla tipologia di propulsione che utilizza."@it ,
                                     "Associates a Vehicle with its propulsion kind."@en ;
                        rdfs:label "ha tipologia di propulsione"@it ,
                                   "has propulsion kind"@en .


###  http://www.w3id.org/urban-iot/sharing#hasSharingMobilityCategory
uiots:hasSharingMobilityCategory rdf:type owl:ObjectProperty ;
                                 rdfs:range uiots:SharingMobilityCategory ;
                                 vann:usageNote "Usato per specificare la categoria di un Servizio di Sharing  Mobility o di una Stazione di Sharing."@it ;
                                 rdfs:comment "Associa una specifica categoria di Sharing Mobility."@it ,
                                              "Associates a specific Sharing Mobility category."@en ;
                                 rdfs:label "ha categoria sharing mobility"@it ,
                                            "has sharing mobility category"@en .


###  http://www.w3id.org/urban-iot/sharing#hasStartingStation
uiots:hasStartingStation rdf:type owl:ObjectProperty ;
                         rdfs:domain uiots:SharingMobilityTrip ;
                         rdfs:range uiots:SharingStation ;
                         rdfs:comment "Associa un Noleggio alla Stazione di Sharing in cui viene iniziato."@it ,
                                      "Associates a Sharing Mobility Trip with the Sharing Station in which it starts."@en ;
                         rdfs:label "ha stazione di inizio"@it ,
                                    "has starting station"@en .


###  http://www.w3id.org/urban-iot/sharing#hasTripEndingPoint
uiots:hasTripEndingPoint rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf uiots:hasTripPoint ;
                         rdfs:domain uiots:SharingMobilityTrip ;
                         rdfs:range uiots:TripPoint ;
                         rdfs:comment "Associa un Noleggio al suo punto di partenza."@it ,
                                      "Associates a Sharing Mobility Trip with its ending point."@en ;
                         rdfs:label "ha punto di inizio noleggio"@it ,
                                    "has trip ending point"@en .


###  http://www.w3id.org/urban-iot/sharing#hasTripPoint
uiots:hasTripPoint rdf:type owl:ObjectProperty ;
                   rdfs:domain uiots:SharingMobilityTrip ;
                   rdfs:range uiots:TripPoint ;
                   rdfs:comment "Associa un Noleggio ad un punto registrato durante il percorso."@it ,
                                "Associates a Sharing Mobility Trip with a point registered during the ride."@en ;
                   rdfs:label "ha punto del noleggio"@it ,
                              "has trip point"@en .


###  http://www.w3id.org/urban-iot/sharing#hasTripRoute
uiots:hasTripRoute rdf:type owl:ObjectProperty ;
                   rdfs:domain uiots:SharingMobilityTrip ;
                   rdfs:range uiots:TripRoute ;
                   rdfs:comment "Associa un Noleggio ad una descrizione spaziale del tragitto percorso."@it ,
                                "Associates a Sharing Mobility Trip with a spatial representation of the route taken."@en ;
                   rdfs:label "ha tragitto noleggio"@it ,
                              "has trip route"@en .


###  http://www.w3id.org/urban-iot/sharing#hasTripStartingPoint
uiots:hasTripStartingPoint rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf uiots:hasTripPoint ;
                           rdfs:domain uiots:SharingMobilityTrip ;
                           rdfs:range uiots:TripPoint ;
                           rdfs:comment "Associa un Noleggio al suo punto di arrivo."@it ,
                                        "Associates a Sharing Mobility Trip with its starting point."@en ;
                           rdfs:label "ha punto di fine noleggio"@it ,
                                      "has trip starting point"@en .


###  http://www.w3id.org/urban-iot/sharing#hasVehicleCategory
uiots:hasVehicleCategory rdf:type owl:ObjectProperty ;
                         rdfs:domain schema:Vehicle ;
                         rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                             owl:hasValue vh-kos:vehicle-category
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                         rdfs:comment "Associa un Veicolo alla sua categoria di veicolo secondo la classificazione internazionale."@it ,
                                      "Associates a Vehicle to its vehicle category according to the international classification."@en ;
                         rdfs:label "ha categoria veicolo"@it ,
                                    "has vehicle category"@en .


###  http://www.w3id.org/urban-iot/sharing#isDockOf
uiots:isDockOf rdf:type owl:ObjectProperty ;
               rdfs:domain uiots:Dock ;
               rdfs:range uiots:PhysicalSharingStation ;
               rdfs:comment "Associa uno Stallo alla Stazione di Sharing in cui si trova."@it ,
                            "Associates a Dock to the Sharing Station it belongs to."@en ;
               rdfs:label "is dock of"@en ,
                          "è stallo in"@it .


###  http://www.w3id.org/urban-iot/sharing#latestSharingVehicleEvent
uiots:latestSharingVehicleEvent rdf:type owl:ObjectProperty ;
                                rdfs:domain uiots:SharingMobilityVehicleRecord ;
                                rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                                    owl:hasValue sh-kos:vehicle-event
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                                rdfs:comment "Property referring to the latest event registered for the Sharing Mobility Vehicle."@en ,
                                             "Proprietà che identifica l'ultimo evento registrato per il Veicolo di Sharing Mobility."@it ;
                                rdfs:label "latest sharing vehicle event"@en ,
                                           "ultimo evento veicolo in sharing"@it .


###  http://www.w3id.org/urban-iot/sharing#makesVehicleAvailable
uiots:makesVehicleAvailable rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf uiot:makesAvailable ;
                            owl:inverseOf uiots:vehicleMadeAvailableBy ;
                            rdfs:domain uiots:SharingMobilityService ;
                            rdfs:range uiots:SharingMobilityVehicle ;
                            rdfs:comment "Associa un Servizio di Sharing Mobility al Veicolo di Sharing Mobility che rende disponibile."@it ,
                                         "Associates a Sharing Mobility Service with a Sharing Mobility Vehicle made available."@en ;
                            rdfs:label "makes vehicle available"@en ,
                                       "rende disponibile veicolo"@it .


###  http://www.w3id.org/urban-iot/sharing#occupiedByVehicle
uiots:occupiedByVehicle rdf:type owl:ObjectProperty ;
                        rdfs:domain uiots:DockRecord ;
                        rdfs:range uiots:SharingMobilityVehicle ;
                        rdfs:comment "Identifica il Veicolo di Sharing Mobility che occupa lo Stallo."@it ,
                                     "Property referring to the Sharing Mobility Vehicle currently parked in the Dock."@en ;
                        rdfs:label "occupato dal veicolo"@it ,
                                   "occupied by vehicle"@en .


###  http://www.w3id.org/urban-iot/sharing#performsTrip
uiots:performsTrip rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf uiot:performsSession ;
                   owl:inverseOf uiots:tripPerformedBy ;
                   rdfs:domain uiot:ServicePrivateUser ;
                   rdfs:range uiots:SharingMobilityTrip ;
                   rdfs:comment "Associa un Utente Privato del Servizio ad un Noleggio effettuato."@it ,
                                "Associates a Service Private User with a Sharing Mobility Trip performed."@en ;
                   rdfs:label "effettua noleggio"@it ,
                              "performs trip"@en .


###  http://www.w3id.org/urban-iot/sharing#providesSharingMobilityService
uiots:providesSharingMobilityService rdf:type owl:ObjectProperty ;
                                     rdfs:domain uiots:SharingMobilityProvider ;
                                     rdfs:range uiots:SharingMobilityService ;
                                     rdfs:comment "Associa un fornitore di servizi di Sharing Mobility ad un servizio."@it ,
                                                  "Associates a Sharing Mobility Provider with a Sharing Mobility Service operated."@en ;
                                     rdfs:label "eroga servizio sharing mobility"@it ,
                                                "provides sharing mobility service"@en .


###  http://www.w3id.org/urban-iot/sharing#relatedDockRecord
uiots:relatedDockRecord rdf:type owl:ObjectProperty ;
                        rdfs:domain uiots:SharingStationRecord ;
                        rdfs:range uiots:DockRecord ;
                        rdfs:comment "Associa un Record Stazione di Sharing al record di uno degli stalli contenuti nella stazione."@it ,
                                     "Associates a Sharing Station Record to a record of one the docks contained in the station."@en ;
                        rdfs:label "record stallo collegato"@it ,
                                   "related dock record"@en .


###  http://www.w3id.org/urban-iot/sharing#sharingStationUsedBy
uiots:sharingStationUsedBy rdf:type owl:ObjectProperty ;
                           owl:inverseOf uiots:usesSharingStation ;
                           rdfs:domain uiots:SharingStation ;
                           rdfs:range uiots:SharingMobilityService ;
                           rdfs:comment "Associa una Stazione di Sharing al Servizio di Sharing Mobility che la utilizza."@it ,
                                        "Associates a Sharing Station with a Sharing Mobility Service using it."@en ;
                           rdfs:label "sharing station used by"@en ,
                                      "stazione di sharing utilizzata da"@it .


###  http://www.w3id.org/urban-iot/sharing#sharingVehicleState
uiots:sharingVehicleState rdf:type owl:ObjectProperty ;
                          rdfs:domain uiots:SharingMobilityVehicleRecord ;
                          rdfs:range [ owl:intersectionOf ( <http://www.w3.org/2004/02/skos/core#Concept>
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                              owl:hasValue sh-kos:vehicle-state
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                          rdfs:comment "Property referring to the current state of a Sharing Mobility Vehicle."@en ,
                                       "Proprietà che indica lo stato in cui si trova attualmente il Veicolo di Sharing Mobility."@it ;
                          rdfs:label "sharing vehicle state"@en ,
                                     "stato del veicolo in sharing"@it .


###  http://www.w3id.org/urban-iot/sharing#tripEnabledBy
uiots:tripEnabledBy rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf uiot:sessionEnabledBy ;
                    rdfs:domain uiots:SharingMobilityTrip ;
                    rdfs:range uiots:SharingMobilityService ;
                    rdfs:comment "Associa un Noleggio al Servizio di Sharing Mobility che lo abilita."@it ,
                                 "Associates a Sharing Mobility Trip with the Sharing Mobility Service enabling it."@en ;
                    rdfs:label "noleggio abilitato da"@it ,
                               "trip enabled by"@en .


###  http://www.w3id.org/urban-iot/sharing#tripPerformedBy
uiots:tripPerformedBy rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf uiot:sessionPerformedBy ;
                      rdfs:domain uiots:SharingMobilityTrip ;
                      rdfs:range uiot:ServicePrivateUser ;
                      rdfs:comment "Associa un Noleggio all'Utente del Servizio che lo effettua."@it ,
                                   "Associates a Sharing Mobility Trip with the Service Private User performing it."@en ;
                      rdfs:label "noleggio effettuato da"@it ,
                                 "trip performed by"@en .


###  http://www.w3id.org/urban-iot/sharing#usesSharingStation
uiots:usesSharingStation rdf:type owl:ObjectProperty ;
                         rdfs:domain uiots:SharingMobilityService ;
                         rdfs:range uiots:SharingStation ;
                         rdfs:comment "Associa un Servizio di Sharing Mobility ad una Stazione di Sharing che utilizza."@it ,
                                      "Associates a Sharing Mobility Service with a Sharing Station used by the service."@en ;
                         rdfs:label "uses sharing station"@en ,
                                    "utilizza stazione di sharing"@it .


###  http://www.w3id.org/urban-iot/sharing#usesVehicle
uiots:usesVehicle rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf uiot:usesResource ;
                  owl:inverseOf uiots:vehicleUsedIn ;
