    <!-- http://w3id.org/urban-iot/kos/vehicle/vehicle-category#CategoryL -->


    <NamedIndividual rdf:about="http://w3id.org/urban-iot/kos/vehicle/vehicle-category#CategoryL">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:definition xml:lang="it">Categoria di veicoli ciclomotori e motoveicoli con meno di quattro ruote e quadricicli leggeri.</skos:definition>
        <skos:definition xml:lang="en">Category of vehicles mopeds and motorcycles with less than four wheels and light quadricycles.</skos:definition>
        <skos:inScheme rdf:resource="http://w3id.org/urban-iot/kos/vehicle/vehicle-category"/>
        <skos:narrower rdf:resource="http://w3id.org/urban-iot/kos/vehicle/vehicle-category#CategoryL6"/>
        <skos:narrower rdf:resource="http://w3id.org/urban-iot/kos/vehicle/vehicle-category#CategoryL7"/>
        <skos:prefLabel xml:lang="it">Categoria L</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Category L</skos:prefLabel>
        <skos:topConceptOf rdf:resource="http://w3id.org/urban-iot/kos/vehicle/vehicle-category"/>
    </NamedIndividual>
    


    <!-- http://w3id.org/urban-iot/kos/vehicle/vehicle-category#CategoryL6 -->


    <NamedIndividual rdf:about="http://w3id.org/urban-iot/kos/vehicle/vehicle-category#CategoryL6">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://w3id.org/urban-iot/kos/vehicle/vehicle-category#CategoryL"/>
        <skos:definition xml:lang="en">Four wheels light vehicles whose unladen mass is not more than 350 kg, whose maximum design speed is not more than 45 km/h, and whose engine cylinder capacity does not exceed 50 cm3, or whose maximum net power output does not exceed 4 kW.</skos:definition>
        <skos:definition xml:lang="it">Quadricicli leggeri con massa a vuoto inferiore o pari a 350 kg, velocità massima inferiore o pari a 45 km/h, cilindrata inferiore o pari a 50cm3, potenza netta/nominale continua massima inferiore o uguale a 4kW.</skos:definition>
        <skos:inScheme rdf:resource="http://w3id.org/urban-iot/kos/vehicle/vehicle-category"/>
        <skos:prefLabel xml:lang="it">Categoria L6</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Category L6</skos:prefLabel>
    </NamedIndividual>
    


    <!-- http://w3id.org/urban-iot/kos/vehicle/vehicle-category#CategoryL7 -->


    <NamedIndividual rdf:about="http://w3id.org/urban-iot/kos/vehicle/vehicle-category#CategoryL7">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:broader rdf:resource="http://w3id.org/urban-iot/kos/vehicle/vehicle-category#CategoryL"/>
        <skos:definition xml:lang="en">Four wheels light vehicles, other than that classified for the category L6, whose unladen mass is not more than 400 kg (550 kg for vehicles intended for carrying goods), and whose maximum continuous rated power does not exceed 15 kW.</skos:definition>
        <skos:definition xml:lang="it">Quadricicli leggeri che non appartengono a L6 con massa a vuoto inferiore o pari a 400 kg (550 kg se per trasporto merci) e con potenza massima inferiore o uguale a 15 kW.</skos:definition>
        <skos:inScheme rdf:resource="http://w3id.org/urban-iot/kos/vehicle/vehicle-category"/>
        <skos:prefLabel xml:lang="it">Categoria L7</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Category L7</skos:prefLabel>
    </NamedIndividual>
    


    <!-- http://w3id.org/urban-iot/kos/vehicle/vehicle-category#CategoryM -->


